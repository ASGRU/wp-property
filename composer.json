{
  "name": "usabilitydynamics/wp-property",
  "description": "Property and Real Estate Management Plugin for WordPress.  Create a directory of real estate / rental properties and integrate them into you WordPress CMS.",
  "version": "2.0.0",
  "type": "plugin",
  "license": "GPL2",
  "minimum-stability": "dev",
  "homepage": "http://github.com/UsabilityDynamics/wp-property",
  "author": {
    "name": "UsabilityDynamics, Inc.",
    "url": "https://usabilitydynamics.com/"
  },
  "keywords": [
    "wordpress",
    "realty",
    "real estate"
  ],
  "support": {
    "issues": "https://github.com/UsabilityDynamics/wp-property/issues",
    "forum": "http://UsabilityDynamics/forum"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/UsabilityDynamics/wp-property.git"
  },
  "require": {
    "php": ">=5.3",
    "composer/installers": "~1.0",
    "usabilitydynamics/lib-settings": "master",
    "usabilitydynamics/lib-wp-bootstrap": "1.0.1",
    "usabilitydynamics/lib-ud-api-client": "1.0.0"
  },
  "require-dev": {
    "usabilitydynamics/lib-wp-phpunit": "master",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "classmap": [ "lib/classes" ]
  },
  "archive": {
    "exclude": [
      "node_modules",
      ".gitignore",
      "test",
      "circle.yml",
      "composer.lock",
      "gruntfile.js",
      "package.json"
    ]
  },
  "extra": {
    "schemas": {
      "dependencies" : {
        "modules": {
          "usabilitydynamics/lib-utility": {
            "\\UsabilityDynamics\\Utility": "0.3.4"
          },
          "usabilitydynamics/lib-settings": {
            "\\UsabilityDynamics\\Settings": "0.2.2"
          }
        },
        "plugins": []
      },
      "licenses" : {
        "client" : {
          "screen": {
            "parent": "edit.php?post_type=property",
            "page_title": "l10n.licenses_page_title",
            "menu_title": "l10n.licenses_menu_title"
          }
        },
        "product" : false
      }
    }
  }
}