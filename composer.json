{
  "name": "usabilitydynamics/wp-realty",
  "version": "1.38.2",
  "type": "wordpress-plugin",
  "extra": {
    "installer-name": "wp-property",
    "installer-paths": {
      "vendor/modules/{$name}":       [ "type:wordpress-module" ],
      "vendor/libraries/{$name}":     [ "type:wordpress-library" ],
      "vendor/themes/{$name}":        [ "type:wordpress-theme" ]
    }
  },
  "homepage": "http://github.com/UsabilityDynamics/wp-property",
  "authors": [
    {
      "name": "Andy Potanin",
      "homepage": "http://usabilitydynamics.com"
    },
    {
      "name": "Maxim Peshkov",
      "email": "maxim.peshkov@usabilitydynamics.com",
      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    },
    {
      "name": "Anton Korotkov",
      "email": "anton.korotkov@usabilitydynamics.com",
      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "config": {
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true,
    "autoloader-suffix": "VendorPackages",
    "component-dir": "js/vendor"
  },
  "require": {
    "usabilitydynamics/lib-installer": "master",
    "wp-property/admin-tools": "master",
    "wp-property/exporter": "master"
  },
  "require-dev": {
    "wp-property/importer": "master"
  },
  "minimum-stability": "dev"
}