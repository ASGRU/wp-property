{
  "name": "wp-property/wp-property",
  "type": "wordpress-plugin",
  "minimum-stability": "dev",
  "homepage": "https://www.usabilitydynamics.com/product/wp-property",
  "author": {
    "name": "UsabilityDynamics, Inc.",
    "url": "https://www.usabilitydynamics.com"
  },
  "config": {
    "preferred-install": "dist",
    "github-protocols": [ "ssh" ],
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "require": {
    "php": ">=5.3",
    "usabilitydynamics/lib-settings": "*",
    "usabilitydynamics/lib-wp-bootstrap": "~1.0",
    "usabilitydynamics/lib-ud-api-client": "~1.0",
    "usabilitydynamics/lib-wp-module": "*",
    "usabilitydynamics/lib-wp-list-table": "~1.0",
    "usabilitydynamics/wp-meta-box": "~4.4",
    "usabilitydynamics/wp-meta-box-utilities": "~1.0",
    "UsabilityDynamics/wp-gallery-metabox": "*"
  },
  "require-dev": {
    "usabilitydynamics/lib-wp-phpunit": "master",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "classmap": [ "lib/classes" ]
  },
  "archive": {
    "exclude": [
      "node_modules",
      ".gitignore",
      "test",
      "circle.yml",
      "composer.lock",
      "gruntfile.js",
      "package.json"
    ]
  },
  "extra": {
    "installer-paths": {
      "vendor/plugins/{$name}": [
        "type:wordpress-muplugin",
        "type:wordpress-plugin",
        "type:wpackagist-plugin"
      ],
      "vendor/modules/{$name}": [
        "type:wordpress-module"
      ],
      "vendor/libraries/{$vendor}/{$name}": [
        "type:wordpress-library"
      ]
    },
    "splashes": {
      "install": "static/splashes/install.php"
    },
    "schemas": {
      "dependencies" : {
        "modules": {},
        "plugins": []
      },
      "licenses" : {
        "client" : {
          "slug": "wp-property/wp-property",
          "screen": {
            "parent": "edit.php?post_type=property",
            "page_title": "l10n.licenses_page_title",
            "menu_title": "l10n.licenses_menu_title"
          }
        },
        "product" : false
      }
    }
  }
}
