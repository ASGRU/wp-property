{
  "name": "usabilitydynamics/wp-property",
  "version": "2.0.0",
  "type": "plugin",
  "minimum-stability": "dev",
  "homepage": "http://github.com/UsabilityDynamics/wp-property",
  "author": {
    "name": "UsabilityDynamics, Inc.",
    "url": "https://usabilitydynamics.com/"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/UsabilityDynamics/wp-property.git"
  },
  "require": {
    "php": ">=5.3",
    "usabilitydynamics/lib-settings": "master",
    "usabilitydynamics/lib-wp-bootstrap": "master",
    "usabilitydynamics/lib-ud-api-client": "master"
  },
  "autoload": {
    "classmap": [ "lib/classes" ]
  },
  "scripts": {
    "pre-install-cmd": [
      "NODE_ENV=development npm install"
    ]
  },
  "archive": {
    "exclude": [
      "node_modules",
      ".gitignore",
      "test",
      "circle.yml",
      "composer.lock",
      "gruntfile.js",
      "package.json"
    ]
  }
}